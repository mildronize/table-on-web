{
  "name": "@thaitype/data-viewer",
  "version": "0.0.0",
  "description": "NPM TypeScript Boilerplate with Changesets CLI & GitHub Actions",
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.mjs",
      "default": "./dist/main.js"
    }
  },
  "files": [
    "dist/**",
    "src/**"
  ],
  "scripts": {
    "all": "run-s lint test:ci build",
    "build": "tsup src/main.ts --dts --format esm,cjs",
    "release": "npm run build && changeset publish",
    "format": "prettier -w src",
    "lint": "tsc --noEmit && eslint ./src && prettier -c src",
    "lint:fix": "eslint --fix ./src && prettier -w src",
    "tsx": "tsx",
    "test": "vitest",
    "test:ci": "vitest run"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/express": "^4.17.21",
    "@types/node": "^18.11.18",
    "esbuild-register": "^3.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "tsup": "^6.6.3",
    "tsx": "^4.7.0",
    "typescript": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitest/coverage-v8": "^1.1.3",
    "eslint": "^8.36.0",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "vitest": "^1.1.3"
  },
  "keywords": [],
  "author": "Thada Wangthammang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaitype/data-viewer.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "dayjs": "^1.11.10",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "socket.io": "^4.7.4"
  }
}